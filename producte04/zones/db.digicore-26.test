;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-26.test. root.digicore-26.test (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	  	 IN	NS	ns1.digicore-26.test.
ns1	 	 IN	A	192.168.4.26
server	 IN	A	192.168.4.26
dbserver IN	A	192.168.4.126
data	 IN	CNAME	dbserver.digicore-26.test.
