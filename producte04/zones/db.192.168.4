;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-25.test. root.digicore-25.test. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.digicore-25.test.
25	IN	PTR	server.digicore-25.test.
125	IN	PTR	dbserver.digicore-25.test.
