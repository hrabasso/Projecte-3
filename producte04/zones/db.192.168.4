;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.digicore-26.test. root.digicore-26.test. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns1.digicore-26.test.
26	IN	PTR	server.digicore-26.test.
126	IN	PTR	dbserver.digicore-26.test.
